@model IEnumerable<Entities.Division>

@{
    ViewBag.Title = "View";
}

<h2>View</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { divisionName = item.Name }) |            
        </td>
    </tr>
}

</table>
