@model Entities.Division

@{
    ViewBag.Title = "View";
}

<h2>@Html.DisplayFor(model=> model.Name)</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Teams) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>           
        </tr>
    }

</table>
